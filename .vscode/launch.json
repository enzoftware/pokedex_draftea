  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Pokedex App",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main.dart"
      ]
    },
    {
      "name": "pokedex_draftea (Web)",
      "request": "launch",
      "type": "dart",
      "deviceId": "chrome",
      "program": "lib/main.dart",
      "args": [
        "--web-renderer",
        "html"
      ]
    }
  ]
}